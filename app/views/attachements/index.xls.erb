<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">ID</Data></Cell>
        <Cell><Data ss:Type="String">Name</Data></Cell>
        <Cell><Data ss:Type="String">Release Date</Data></Cell>
        <Cell><Data ss:Type="String">Price</Data></Cell>
      </Row>
    <% @attachements.each do |attachement| %>
      <Row>
        <Cell><Data ss:Type="Number"><%= attachement.fonction %></Data></Cell>
	    <Cell><Data ss:Type="Number"><%= attachement.emplacement %></Data></Cell>
	    <Cell><Data ss:Type="Number"><%= attachement.stock %></Data></Cell>
	    <Cell><Data ss:Type="Number"><%= attachement.marque %></Data></Cell>
	    <Cell><Data ss:Type="Number"><%= attachement.modele %></Data></Cell>
	    <Cell><Data ss:Type="Number"><%= attachement.description %></Data></Cell>
	    <Cell><Data ss:Type="Number"><%= attachement.prix %></Data></Cell>
	    <Cell><Data ss:Type="Number"><%= attachement.bonus %></Data></Cell>        
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>